{
  "garbage_collection": {
    "enabled": true,
    "interval_seconds": 120,
    "thresholds": [500, 5, 5],
    "debug": false,
    "aggressive_mode": true,
    "collect_generation": [0, 1, 2]
  },
  "memory_profiling": {
    "enabled": true,
    "log_interval_seconds": 30,
    "max_memory_mb": 2048,
    "alert_threshold_percent": 75,
    "critical_threshold_percent": 85,
    "auto_optimization": true
  },
  "object_pooling": {
    "enabled": true,
    "max_pool_size": 500,
    "object_types": ["dict", "list"],
    "cleanup_interval": 300,
    "max_object_age": 1800
  },
  "caching": {
    "enabled": true,
    "max_cache_size_mb": 256,
    "ttl_seconds": 1800,
    "strategies": ["lru"],
    "compression": true,
    "lazy_loading": true
  },
  "database_optimization": {
    "connection_pool_size": 5,
    "pool_recycle": 1800,
    "pool_pre_ping": true,
    "max_overflow": 2,
    "statement_cache_size": 50
  },
  "flask_optimization": {
    "max_content_length": 16 * 1024 * 1024,
    "json_sort_keys": false,
    "jsonify_prettyprint": false,
    "send_file_max_age_default": 300,
    "session_cookie_secure": true,
    "session_cookie_httponly": true,
    "session_cookie_samesite": "Lax"
  },
  "socketio_optimization": {
    "max_http_buffer_size": 1 * 1024 * 1024,
    "ping_timeout": 30,
    "ping_interval": 15,
    "max_decode_packets": 16,
    "compression_threshold": 1024
  },
  "logging_optimization": {
    "log_level": "WARNING",
    "max_log_size_mb": 100,
    "log_backup_count": 3,
    "log_compression": true,
    "async_logging": true
  }
}